<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sessions_requested_between_range</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Sessions_Requested__c &lt; 2,Sessions_Requested__c &gt; 50)</errorConditionFormula>
    <errorDisplayField>Sessions_Requested__c</errorDisplayField>
    <errorMessage>Sessions requested can only be between 2-50</errorMessage>
</ValidationRule>
